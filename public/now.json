{
    "version": 2,
    "alias": "pickup-game",
    "name": "pickup-game",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/manifest.json$",
          "dest": "/manifest.json"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }